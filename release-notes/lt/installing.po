#~ msgid ""
#~ "brltty=<replaceable>driver</replaceable>,<replaceable>device</"
#~ "replaceable>,<replaceable>table</replaceable>"
#~ msgstr ""
#~ "brltty=<replaceable>driver</replaceable>,<replaceable>device</"
#~ "replaceable>,<replaceable>table</replaceable>"

