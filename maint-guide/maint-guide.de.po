msgstr ""

#. type: <p></p>
#: maint-guide.en.sgml:2969
msgid "downgrade it back to the previous version (optional)."
msgstr ""

#. type: <p></p>
#: maint-guide.en.sgml:2970 maint-guide.en.sgml:2974
msgid "purge it."
msgstr ""

#. type: <p></p>
#: maint-guide.en.sgml:2971
#, fuzzy
msgid "install the new package."
