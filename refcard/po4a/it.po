msgstr "dpkg"

# type: Content of: <article><articleinfo><keywordset><keyword>
#: entries.dbk:35
msgid "reference card"
msgstr "scheda di riferimento"

# type: Content of: <article><articleinfo><keywordset><keyword>
#: entries.dbk:36
msgid "basic commands"
msgstr "comandi di base"

# type: Content of: <article><articleinfo><keywordset><keyword>
#: entries.dbk:37
