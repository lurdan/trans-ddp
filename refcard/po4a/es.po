#: entries.dbk:274
msgid "Move, rename files."
msgstr "Mueve/renombra archivos."

#. type: Content of: <article><section><glosslist><glossentry><glossterm>
#: entries.dbk:277
msgid "rm <replaceable>files</replaceable>"
msgstr "rm <replaceable>archivos</replaceable>"

#. type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:278
msgid "Remove files."
msgstr "Elimina archivos."

#. type: Content of: <article><section><glosslist><glossentry><glossterm>
#: entries.dbk:281
