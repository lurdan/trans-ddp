msgstr "<glossterm><ulink url=\"http://www.debian.org/devel/debian-installer/\">Installer</ulink></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:84
msgid "All information about it at <literal>http://www.debian.org/devel/debian-installer/</literal>"
msgstr "Alle Informationen dazu unter <literal>http://www.debian.org/devel/debian-installer/</literal>"

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:88
msgid "<glossterm><ulink url=\"http://www.debian.org/distrib/\">CD images</ulink></glossterm>"
msgstr "<glossterm><ulink url=\"http://www.debian.org/distrib/\">CD-Images</ulink></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:91
msgid "Download from <literal>http://www.debian.org/distrib/</literal>"
msgstr "Laden von <literal>http://www.debian.org/distrib/</literal>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:96
msgid "E.g. to set up the network w/o DHCP or using LILO instead of GRUB."
msgstr "Z.B. zum Konfigurieren des Netzwerkes ohne DHCP oder zum Nutzen von LILO statt GRUB."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:100
msgid "<glossterm><literal>boot: linux26</literal> or <literal>boot: expert26</literal></glossterm>"
msgstr "<glossterm><literal>boot: linux26</literal> oder <literal>boot: expert26</literal></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:102
msgid "Use Linux kernel 2.6 for installation."
msgstr "Linux kernel 2.6 für die Installation verwenden."

# type: Content of: <article><section><title>
#: entries.dbk:108
msgid "Bugs"
msgstr "Bugs"

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:111
msgid "<glossterm><ulink url=\"http://bugs.debian.org/\">Bug Tracking</ulink> at <literal>http://bugs.debian.org/</literal></glossterm>"
msgstr "<glossterm><ulink url=\"http://bugs.debian.org/\">Fehlerdatenbank</ulink> unter <literal>http://bugs.debian.org/</literal></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:114
msgid "All about existing and fixed bugs."
msgstr "Alles über bekannte und gelöste Fehler."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:118
msgid "<glossterm>Package specific</glossterm>"
msgstr "<glossterm>Bestimmtes Paket</glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:119
msgid "See <literal>http://bugs.debian.org/<replaceable>package-name</replaceable>/</literal>, use <literal>wnpp</literal> to ask for new packages."
msgstr "Siehe <literal>http://bugs.debian.org/<replaceable>Paket-Name</replaceable></literal>, benutze <literal>wnpp</literal> um neue Pakete anzufragen."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:125
msgid "<glossterm>reportbug</glossterm>"
msgstr "<glossterm>reportbug</glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:126
msgid "Report a bug by e-mail."
msgstr "Fehler per E-Mail berichten."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:129
msgid "<glossterm><ulink url=\"http://www.debian.org/Bugs/Reporting\">Reporting</ulink></glossterm>"
msgstr "<glossterm><ulink url=\"http://www.debian.org/Bugs/Reporting\">Fehler berichten</ulink></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:131
msgid "Instructions at <literal>http://www.debian.org/Bugs/Reporting</literal>"
msgstr "Anweisungen zum Berichten von Fehlern unter <literal>http://www.debian.org/Bugs/Reporting</literal>"

# type: Content of: <article><section><title>
#: entries.dbk:137
msgid "Configuration"
msgstr "Konfiguration"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:141
msgid "All system configuration files are under directory <filename>/etc/</filename>."
msgstr "Alle Konfigurations-Dateien des Systems befinden sich im Verzeichnis <filename>/etc/</filename>."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:145
msgid "<glossterm>nano <replaceable>files</replaceable></glossterm>"
msgstr "<glossterm>nano <replaceable>Dateien</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:146
msgid "Default text editor.  If not present, try <command>emacs</command>, <command>vi</command>, <command>joe</command>."
msgstr "Standard Text-Editor. Falls nicht vorhanden, probieren Sie <command>emacs</command>, <command>vi</command> oder <command>joe</command>."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:151
msgid "<glossterm><ulink url=\"https://localhost:10000\">webmin</ulink> at <literal>https://hostname:10000</literal></glossterm>"
msgstr "<glossterm><ulink url=\"https://localhost:10000\">webmin</ulink> unter <literal>https://hostname:10000</literal></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:153
msgid "Browser interface to system configuration, access is defined in <filename>/etc/webmin/miniserv.conf</filename>."
msgstr "Browser-Oberfläche zur System-Konfiguration.  Zugriffsrechte sind in <filename>/etc/webmin/miniserv.conf</filename> definiert."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:158
msgid "<glossterm><ulink url=\"http://localhost:631\">CUPS</ulink> at <literal>http://hostname:631</literal></glossterm>"
msgstr "<glossterm><ulink url=\"http://localhost:631\">CUPS</ulink> unter <literal>http://hostname:631</literal></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:160
msgid "Browser interface to printing system."
msgstr "Browser-Oberfläche zur Drucker-Konfiguration."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:164
msgid "<glossterm>dpkg-reconfigure <replaceable>package-name</replaceable></glossterm>"
msgstr "<glossterm>dpkg-reconfigure <replaceable>Paket-Name</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:166
msgid "Reconfigure a package, e.g. console-common (keyboard), locales (localisation)."
msgstr "Rekonfigurieren eines Paketes, z.B. console-common (Tastatur), locales (Sprache)."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:170
msgid "<glossterm>update-alternatives <replaceable>options</replaceable></glossterm>"
msgstr "<glossterm>update-alternatives <replaceable>Optionen</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:172
msgid "Manage command alternatives."
msgstr "Konfiguration von Kommando-Alternativen."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:175
msgid "<glossterm>update-grub</glossterm>"
msgstr "<glossterm>update-grub</glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:176
msgid "After installing a new kernel."
msgstr "Nach Installation eines neuen Kernels."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:180
msgid "<glossterm>make-kpkg <literal>--</literal>initrd <literal>--</literal>revision=2:my.1.0 <literal>--</literal>rootcmd fakeroot <literal>--</literal>uc <literal>--</literal>us <literal>kernel_image</literal></glossterm>"
msgstr "<glossterm>make-kpkg <literal>--</literal>initrd <literal>--</literal>revision=2:my.1.0 <literal>--</literal>rootcmd fakeroot <literal>--</literal>uc <literal>--</literal>us kernel_image</glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:185
msgid "Build a kernel packages from sources, if a customised kernel is really needed, needs <literal>kernel-package</literal>."
msgstr "Ein Kernel-Paket aus den Quellen bauen, wenn ein angepasster Kernel wirklich gebraucht wird.  Benötigt <literal>kernel-package</literal>."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:190
msgid "<glossterm>m-a a-i module <literal>kernel_image</literal></glossterm>"
msgstr "<glossterm>m-a a-i module kernel_image</glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:192
msgid "Create and install third-party modules (nvidia, ...), needs <literal>module-assistant</literal>."
msgstr "Erzeuge und installiere Fremd-Module (nvidia, ...), benötigt <literal>module-assistant</literal>."

# type: Content of: <article><section><title>
#: entries.dbk:199
msgid "Daemons and System"
msgstr "Dämonen und System"

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:202
msgid "<glossterm><filename>/etc/init.d/<replaceable>file</replaceable> restart</filename></glossterm>"
msgstr "<glossterm><filename>/etc/init.d/<replaceable>Datei</replaceable> restart</filename></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:204
msgid "Restart a service, system daemon."
msgstr "Neustarten eines Dienstes oder System-Dämons."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:208
msgid "<glossterm><filename>/etc/init.d/<replaceable>file</replaceable> stop</filename></glossterm>"
msgstr "<glossterm><filename>/etc/init.d/<replaceable>Datei</replaceable> stop</filename></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:210
msgid "Stop a service, system daemon."
msgstr "Stoppen eines Dienstes oder System-Dämons."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:214
msgid "<glossterm><filename>/etc/init.d/<replaceable>file</replaceable> start</filename></glossterm>"
msgstr "<glossterm><filename>/etc/init.d/<replaceable>Datei</replaceable> start</filename></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:216
msgid "Start a service, system daemon."
msgstr "Starten eines Dienstes oder System-Dämons."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:220
msgid "<glossterm>halt, reboot, poweroff</glossterm>"
msgstr "<glossterm>halt, reboot, poweroff</glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:221
msgid "Halts, reboots, shuts down system."
msgstr "Hält das System an, startet es neu oder schaltet es aus."

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:226
msgid "All log files are under this directory."
msgstr "Hier finden Sie alle Protokoll-Dateien."

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:231
msgid "Default values for many daemons and services."
msgstr "Standard-Werte für viele Dämonen und Dienste."

# type: Content of: <article><section><title>
#: entries.dbk:237
msgid "Important Shell Commands"
msgstr "Wichtige Shell-Kommandos"

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:240
msgid "<glossterm>cat <replaceable>files</replaceable></glossterm>"
msgstr "<glossterm>cat <replaceable>Dateien</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:241
msgid "Print files to screen."
msgstr "Ausgeben der Dateien."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:244
msgid "<glossterm>cd <replaceable>directory</replaceable></glossterm>"
msgstr "<glossterm>cd <replaceable>Verzeichnis</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:245
msgid "Change to directory."
msgstr "Wechsle das Verzeichnis"

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:248
msgid "<glossterm>cp <replaceable>files</replaceable> <replaceable>dest</replaceable></glossterm>"
msgstr "<glossterm>cp <replaceable>Dateien</replaceable> <replaceable>Ziel</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:250
msgid "Copy files and directories."
msgstr "Kopiere Dateien und Verzeichnisse."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:253
msgid "<glossterm>echo <replaceable>string</replaceable></glossterm>"
msgstr "<glossterm>echo <replaceable>Text</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:254
msgid "Echo string to screen."
msgstr "Echo des Textes."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:257
msgid "<glossterm>gzip, bzip2 <optional><literal>-d</literal></optional> <replaceable>files</replaceable></glossterm>"
msgstr "<glossterm>gzip, bzip2 <optional><literal>-d</literal></optional> <replaceable>Dateien</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:260
msgid "Compress, uncompress files."
msgstr "Komprimiere, dekomprimiere Dateien."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:263
msgid "<glossterm>less <replaceable>files</replaceable></glossterm>"
msgstr "<glossterm>less <replaceable>Dateien</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:264
msgid "Show contents of files."
msgstr "Zeigt den Inhalt von Dateien."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:267
msgid "<glossterm>ls <optional><replaceable>files</replaceable></optional></glossterm>"
msgstr "<glossterm>ls <optional><replaceable>Dateien</replaceable></optional></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:269
msgid "List files."
msgstr "Listet Dateien auf."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:272
msgid "<glossterm>mkdir <replaceable>directory-names</replaceable></glossterm>"
msgstr "<glossterm>mkdir <replaceable>Verzeichnis-Namen</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:274
msgid "Create directories."
msgstr "Erzeuge Verzeichnisse."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:277
msgid "<glossterm>mv <replaceable>file1</replaceable> <literal> </literal> <replaceable>file2</replaceable></glossterm>"
msgstr "<glossterm>mv <replaceable>Datei1</replaceable> <replaceable>Datei2</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:279
msgid "Move, rename files."
msgstr "Verschiebe Dateien und/oder benenne sie um."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:282
msgid "<glossterm>rm <replaceable>files</replaceable></glossterm>"
msgstr "<glossterm>rm <replaceable>Dateien</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:283
msgid "Remove files."
msgstr "Lösche Dateien."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:286
msgid "<glossterm>rmdir <replaceable>dirs</replaceable></glossterm>"
msgstr "<glossterm>rmdir <replaceable>Verzeichnisse</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:287
msgid "Remove empty directories."
msgstr "Lösche leere Verzeichnisse."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:290
msgid "<glossterm>tar <optional>c</optional><optional>x</optional><optional>t</optional><optional>z</optional><optional>j</optional> -f <replaceable>file</replaceable>.tar <literal> </literal> <optional><replaceable>files</replaceable></optional></glossterm>"
msgstr "<glossterm>tar <optional>c</optional><optional>x</optional><optional>t</optional><optional>z</optional><optional>j</optional> <replaceable>-f Archiv-Datei</replaceable> <optional><replaceable>Dateien</replaceable></optional></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:294
msgid "<emphasis>C</emphasis>reate, e<emphasis>X</emphasis>tract, list <emphasis>T</emphasis>able of archive file, <emphasis>z</emphasis>, <emphasis>j</emphasis> for <filename>.gz</filename>, <filename>.bz2</filename>."
msgstr "Erzeuge (c), entpacke (x) oder liste den Inhalt (t) einer Archiv-Datei, z/j für .gz/.bz2."

# type: Content of: <article><section><glosslist><glossentry>
#: entries.dbk:301
msgid "<glossterm>find <replaceable>directories expressions</replaceable></glossterm>"
msgstr "<glossterm>find <replaceable>Verzeichnisse Ausdruck</replaceable></glossterm>"

# type: Content of: <article><section><glosslist><glossentry><glossdef><para>
#: entries.dbk:303
msgid "Find files like <literal>-name <replaceable>name</replaceable></literal> or <literal>-size <replaceable>+1000</replaceable></literal>, etc."
msgstr "Finde passende Dateien (z.B. <literal>-name</literal> oder <literal>-size</literal>)"

