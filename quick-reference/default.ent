<!--  ( This is taken from boot-floppy )

These variables are read and used by all qref.??.sgml; they are included
separate from the main document since they may need to change.  This
allows builders to override the defaults without editing the main SGML
file.

These entries should be language independent

Remember, this is SGML; the *first* definition of an entity wins.
Thus dynamic.ent overrules this one.

-->

<!-- Default values overridden by dynamic.ent -->

<!entity % FIXME "IGNORE">
<!entity % lang-en "IGNORE">
<!entity % lang-fr "IGNORE">
<!entity % lang-it "IGNORE">
<!entity % lang-ja "IGNORE">
<!entity % qref "IGNORE">
<!entity % qref-short "IGNORE">

<!-- update these by each release -->
<!entity version "Version: 1.10p00">


<!-- Make these language dependent using lang-?? start -->

<![ %lang-fr [
<!entity book-title "Guide de référence pour Debian"> 
]]>
<![ %lang-it [
<!entity book-title "Debian: la guida"> 
]]>
<!-- if not defined, use English -->
<!entity book-title "Debian reference">

<!-- Make these language dependent using lang-?? end -->

<!-- URLs -->

<!entity qrefhome       "http://qref.sf.net/">
<!entity qref1home      "http://www.debian.org/doc/manuals/quick-reference/">
<!entity qref2home      "http://qref.sf.net/quick2/">
<!entity example1home   "examples/">
<!entity example2home   "examples/">
<!entity qref1tarfile   "qref.tar.gz">
<!entity qref2tarfile   "http://qref.sf.net/quick2/qref2.tar.gz">
<!entity debianhome     "http://www.debian.org/">
<!entity debianjphome   "http://www.debian.or.jp/">


<!-- proper (long) name to use for Debian -->

<!entity debian "Debian GNU/Linux">
<!entity newdebian "Debian Woody (3.0)">
<!entity gnulinux "GNU/Linux">
<!entity linuxkernel "Linux kernel">
<!entity linuxconsole "Linux console">
<!entity unix "Unix">

<!entity release                     "2.2">
<!entity developers                  "800">
<!entity all-pkgs                    "4200">
<!entity main-pkgs                   "3750">
<!entity contrib-nonfree-pkgs        "450">



<!-- name for the enter key -->
<!entity enterkey '<em>Enter</em>'>


<!entity ldphome        "http://www.linuxdoc.org/">
<!entity ldp            "Linux Documentation Project (LDP)">
<!entity ddphome        "http://www.debian.org/doc/ddp">
<!entity ddp            "Debian Documentation Project (DDP)">
<!entity debinsthome    "http://www.debian.org/releases/stable/installmanual">
<!entity dedinst        "Debian Install Manual">
<!entity howtopackage   "doc-linux-text">
<!entity dochome        "/usr/share/doc/">
<!entity howtohome      "&dochome;HOWTO/en-txt/">
<!entity linonlap       "http://www.linux-laptop.net/">

<!-- Keep this comment at the end of the file
Local variables:
mode: sgml
End:
-->
