<!ENTITY language "@@LANGS@@">

]>
<!-- CVS revision of this document "$Revision: 1.30 $"  -->

<!--
        document conventions:
          file,directory,device == file (full path)
          program, command == prgn
          variable == var
          package == package
          program output == tt
          key input == tt, example for longer ones
          command input == tt
          book title == em
          emphasis == strong
	  
          my todo items: "less than mark" ![ &percnt;FIXME [ ... ]] "larger than mark"

	must be less than 72 char (70 better) per line within example tag
-->

<debiandoc>

	<book id="&book-titleid;">
                               <!-- f-ref / q-ref/ filename -->
		&titletoc;     <!-- ----- / ---- / titletoc.sgml -->
		&copyleft;     <!-- ----- / ---- / copyleft.sgml -->
		<toc sect1>
		&ch-preface;   <!-- Ch  1 / Ch 1 / preface.sgml -->
<![%f-ref;[
		&ch-system;    <!-- Ch  2 / ---- / system.sgml -->
		&ch-install;   <!-- Ch  3 / ---- / install.sgml -->
		&ch-tutorial;  <!-- Ch  4 / ---- / tutorial.sgml -->
]]>
		&ch-woody;     <!-- Ch  5 / Ch 2 / woody.sgml -->
		&ch-debian;    <!-- Ch  6 / Ch 3 / debian.sgml -->
<![%f-ref;[
		&ch-kernel;    <!-- Ch  7 / ---- / kernel.sgml -->
		&ch-tips;      <!-- Ch  8 / ---- / tips.sgml -->
		&ch-tune;      <!-- Ch  9 / ---- / tune.sgml -->
		&ch-gateway;   <!-- Ch 10 / ---- / gateway.sgml -->
		&ch-edit;      <!-- Ch 11 / ---- / edit.sgml -->
		&ch-cvs;       <!-- Ch 12 / ---- / cvs.sgml -->
		&ch-program;   <!-- Ch 13 / ---- / program.sgml -->
		&ch-gnupg;     <!-- Ch 14 / ---- / gnupg.sgml -->
		&ch-support;   <!-- Ch 15 / ---- / support.sgml -->
]]>
		&ch-append;    <!-- Appendix     / append.sgml -->
	</book>

</debiandoc>
