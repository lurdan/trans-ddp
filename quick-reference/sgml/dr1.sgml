<!ENTITY ch-cvs		SYSTEM "@@DIRS@@/cvs.sgml">
<!ENTITY ch-program	SYSTEM "@@DIRS@@/program.sgml">
<!ENTITY ch-gnupg	SYSTEM "@@DIRS@@/gnupg.sgml">
