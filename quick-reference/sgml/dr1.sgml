<!ENTITY % default  SYSTEM "dr1.ent">  %default;
<!ENTITY titletoc	SYSTEM "@@DIRS@@/titletoc.sgml">
<!ENTITY copyleft	SYSTEM "@@DIRS@@/copyleft.sgml">
<!ENTITY ch-preface	SYSTEM "@@DIRS@@/preface.sgml">
