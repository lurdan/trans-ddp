#!/bin/sh
while read X ; do
	Y=$(wget -O - $X 2>/dev/null |sed -n "s/^.*interwiki-$1\"><a href=\"\(.*\)\".*$/\1/p")
	if [ -z "$Y" ]; then 
		echo "# ################# no-translaton"
		echo "msgid \"$X\""
		echo "msgstr \"$X\""
		echo ""
		echo "### $X: no-translaton." 1>&2
	else
		echo "# ----------------- translated"
		echo "msgid \"$X\""
		echo "msgstr \"$Y\""
		echo ""
		echo "--- $X: translated." 1>&2
	fi
done

