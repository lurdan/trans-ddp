<!--
        document conventions:
          file,directory,device == file (full path)
          program, command == prgn
          variable == var
          package == package
          program output == tt
          key input == tt, example for longer ones
          command input == tt
          book title == em
          emphasis == strong
	  
          my todo items: <![ %FIXME [ ... ]]>

	must be liess than 72 char (70 better) per line within example tag
-->

<debiandoc>

	<book id="&book-title;">

		&titletoc;
		&copyleft;
		<toc sect1>
		&ch-preface;
<![ %f-ref [
		&ch-system;
		&ch-install;
		&ch-tutorial;
]]>
		&ch-woody;
		&ch-debian;
<![ %f-ref [
		&ch-kernel;
		&ch-tips;
		&ch-tune;
		&ch-gateway;
		&ch-edit;
		&ch-cvs;
		&ch-program;
		&ch-gnupg;
		&ch-support;
]]>
		&ch-append;
	</book>

</debiandoc>
