<!doctype debiandoc public "-//DebianDoc//DTD DebianDoc//EN"
[

	<!-- textual data entities -->
	<!entity % dynamic  system "@@NAME@@-@@LANGS@@.ent"> %dynamic;
	<!entity % custom   system "custom.ent">  %custom;
	<!entity % default   system "default.ent">  %default;
	<!entity titletoc	system "@@LANGS@@/titletoc.sgml">
	<!entity copyleft	system "@@LANGS@@/copyleft.sgml">
	<!entity ch-preface	system "@@LANGS@@/preface.sgml">
	<!entity ch-install	system "@@LANGS@@/install.sgml">
	<!entity ch-tutorial	system "@@LANGS@@/tutorial.sgml">
	<!entity ch-woody	system "@@LANGS@@/woody.sgml">
	<!entity ch-debian	system "@@LANGS@@/debian.sgml">
	<!entity ch-system	system "@@LANGS@@/system.sgml">
	<!entity ch-kernel	system "@@LANGS@@/kernel.sgml">
	<!entity ch-tips	system "@@LANGS@@/tips.sgml">
	<!entity ch-tune	system "@@LANGS@@/tune.sgml">
	<!entity ch-gateway	system "@@LANGS@@/gateway.sgml">
	<!entity ch-edit	system "@@LANGS@@/edit.sgml">
	<!entity ch-cvs		system "@@LANGS@@/cvs.sgml">
	<!entity ch-program	system "@@LANGS@@/program.sgml">
	<!entity ch-gnupg	system "@@LANGS@@/gnupg.sgml">
	<!entity ch-support	system "@@LANGS@@/support.sgml">
	<!entity ch-append	system "@@LANGS@@/append.sgml">

]
>
