#: debian-reference.en.xmlt:20316
msgid "<emphasis role=\"strong\">s</emphasis>ync all mounted filesystems"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:20324
msgid ""
"remount all mounted filesystems read-only (<emphasis "
"role=\"strong\">u</emphasis>mount)"
