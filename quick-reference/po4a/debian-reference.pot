msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:9328
msgid "halt the system"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:9333
msgid "<literal>S</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
