msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:2208
msgid "select and copy to the clipboard"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:2213
msgid "Left-click"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:2216
msgid "select the start of selection"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:2221
msgid "Right-click"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:2224
