# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:28130
#, no-wrap
msgid ""
"$ zcat /usr/share/doc/groff/meintro.me.gz | \\\n"
"    GROFF_NO_SGR=1 groff -Tascii -me - | col -b -x &gt; meintro.txt"
msgstr ""

