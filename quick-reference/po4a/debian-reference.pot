# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:18590
#, no-wrap
msgid "$ sh -i 2&gt;&amp;1 | tee typescript"
msgstr ""

# type: Content of: <book><chapter><section><section><tip><simpara>
#: debian-reference.en.xml:18592
msgid ""
