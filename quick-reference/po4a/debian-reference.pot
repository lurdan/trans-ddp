#: debian-reference.en.xmlt:13528
#, no-wrap
msgid ""
"$ cat /etc/postfix/sasl_passwd\n"
"[smtp.hostname.dom]:587     username:password\n"
"$ sudo postmap hush:/etc/postfix/sasl_passwd"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
