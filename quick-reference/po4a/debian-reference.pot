#. type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:22981
msgid ""
"lightweight, extensible <emphasis role=\"strong\">meta-backup</emphasis> "
"system"
msgstr ""

#. type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:22988
msgid ""
"<ulink url=\"&mondorescue;\">Mondo Rescue</ulink>: disaster recovery backup "
"suite"
msgstr ""

#. type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:22996
msgid "simple backup suite for GNOME desktop"
msgstr ""

#. type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:23004
msgid "backup system for KDE"
msgstr ""

#. type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:23011
msgid ""
"<ulink url=\"&bacula;\">Bacula</ulink>: network backup, recovery and "
"verification - common support files"
msgstr ""

#. type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:23018
msgid ""
"<ulink url=\"&bacula;\">Bacula</ulink>: network backup, recovery and "
"verification - client meta-package"
msgstr ""

#. type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:23025
msgid ""
"<ulink url=\"&bacula;\">Bacula</ulink>: network backup, recovery and "
"verification - text console"
msgstr ""

#. type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:23032
msgid ""
"<ulink url=\"&bacula;\">Bacula</ulink>: network backup, recovery and "
