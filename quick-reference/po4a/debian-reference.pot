# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:6157
msgid "match &lt;term&gt; package with filtered version"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:6160
msgid "<literal>~S filter &lt;term&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:6165
msgid "match all packages (true)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:6168
msgid "<literal>~T</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:6173
msgid "match no packages (false)"
