msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:3513
msgid "file descriptor"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:3520
msgid "<literal>stdin</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:3523
msgid "standard input"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:3526 debian-reference.en.xmlt:8818 debian-reference.en.xmlt:18991
msgid "0"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:3531
msgid "<literal>stdout</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:3534
msgid "standard output"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:3537 debian-reference.en.xmlt:7716 debian-reference.en.xmlt:8829 debian-reference.en.xmlt:10152 debian-reference.en.xmlt:19359
msgid "1"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:3542
msgid "<literal>stderr</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
