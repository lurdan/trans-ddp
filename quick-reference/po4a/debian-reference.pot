msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xml:8189
msgid "Record/copy system configuration"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:8190
