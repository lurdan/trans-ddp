# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:1563
msgid "<literal>/dev/random</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:1569
msgid ""
"it returns random characters from a true random number generator, delivering "
"real entropy. (slow)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
