msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:11223
msgid "the ISP password,"
msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:11228
msgid "the port speed,"
msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:11233
msgid "the modem communication port, and"
msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:11238
msgid "the authentication method."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:11242 debian-reference.en.xml:11333 debian-reference.en.xml:11402
