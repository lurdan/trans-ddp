#. type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:20244
#, no-wrap
msgid "find . -type f -maxdepth 1 -name '*.ext' -print0 | xargs -0 -n 1 command"
msgstr ""

#. type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xmlt:20247
msgid ""
"<literal>find</literal>(1) with \"<literal>-exec</literal>\" option with a "
"command:"
msgstr ""

#. type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:20251
#, no-wrap
msgid "find . -type f -maxdepth 1 -name '*.ext' -exec command '{}' \\;"
msgstr ""

#. type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xmlt:20254
msgid ""
"<literal>find</literal>(1) with \"<literal>-exec</literal>\" option with a "
"short shell script:"
