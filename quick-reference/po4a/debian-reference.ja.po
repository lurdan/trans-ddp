msgstr "SSH version 1での接続を強制します。"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:15097
msgid ""
"<literal>ssh -1 -o RSAAuthentication=no -l username hostname.domain.ext</"
"literal>"
msgstr ""
"<literal>ssh -1 -o RSAAuthentication=no -l username hostname.domain.ext</"
"literal>"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
