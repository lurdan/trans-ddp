# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25449
msgid "* re-add release tag"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25450
#, no-wrap
msgid ""
"$ cd /path/to\n"
"$ cvs co -r Release-initial -d old project-x"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25455
msgid "get original version to \"<literal>/path/to/old</literal>\" directory"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25459
#, no-wrap
msgid ""
"$ cd old\n"
"$ cvs tag -b Release-initial-bugfixes"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25464
msgid "create branch (-b) tag \"<literal>Release-initial-bugfixes</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25469
msgid "now you can work on the old version (Tag is sticky)"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25473
#, no-wrap
msgid "$ cvs update -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25477
msgid "don't create empty directories"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25482
msgid "source tree now has sticky tag \"Release-initial-bugfixes\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25487
msgid "work on this branch … while someone else making changes too"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25491
#, no-wrap
msgid "$ cvs up -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25495
msgid "sync with files modified by others on this branch"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25499
#, no-wrap
msgid ""
"$ cvs ci -m \"check into this branch\"\n"
"$ cvs update -kk -A -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25504
msgid "remove sticky tag and forget contents"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25509
msgid "update from main trunk without keyword expansion"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25513
#, no-wrap
msgid "$ cvs update -kk -d -P -j Release-initial-bugfixes"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25517
msgid "merge from Release-initial-bugfixes branch into the main"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25522
msgid "trunk without keyword expansion.  Fix conflicts with editor."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25526
#, no-wrap
msgid ""
"$ cvs ci -m \"merge Release-initial-bugfixes\"\n"
"$ cd\n"
"$ tar -cvzf old-project-x.tar.gz old"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25532
msgid ""
"make archive. use \"<literal>-j</literal>\" if you want \"<literal>.tar.bz2</"
"literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25536
#, no-wrap
msgid "$ cvs release -d old"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25540
msgid "remove local source (optional)"
msgstr ""

# type: Content of: <book><chapter><section><section><section><table><title>
#: debian-reference.en.xml:25545
msgid ""
"Notable options for CVS commands (use as first argument(s) to <literal>cvs</"
"literal>(1))."
msgstr ""

#: debian-reference.en.xml:25552
msgid "option"
msgstr "オプション"

# type: Content of: <book><chapter><section><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:25562
msgid "<literal>-n</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:25565
msgid "dry run, no effect"
msgstr ""

# type: Content of: <book><chapter><section><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:25570
msgid "<literal>-t</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:25573
msgid "display messages showing steps of cvs activity"
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25581
msgid "Export files from CVS"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25582
msgid ""
"To get the latest version from CVS, use \"<literal>tomorrow</literal>\":"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25583
#, no-wrap
msgid "$ cvs ex -D tomorrow module_name"
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25586
msgid "Administer CVS"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25587
msgid "Add alias to a project (local server):"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25588
#, no-wrap
msgid ""
"$ export CVSROOT=/var/lib/cvs\n"
"$ cvs co CVSROOT/modules\n"
"$ cd CVSROOT\n"
"$ echo \"px -a project-x\" &gt;&gt;modules\n"
"$ cvs ci -m \"Now px is an alias for project-x\"\n"
"$ cvs release -d .\n"
"$ cvs co -d project px"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25598
msgid "check out project-x (alias:px) from CVS to directory project"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25602
#, no-wrap
msgid "$ cd project"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25610
msgid ""
"In order to perform above procedure, you should have the appropriate file "
"permission."
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25613
msgid "File permissions in repository"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25614
msgid ""
"CVS will not overwrite the current repository file but replaces it with "
"another one.  Thus, write permission to the repository directory is "
"critical.  For every new repository creation, run the following to ensure "
"this condition if needed."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25615
#, no-wrap
msgid ""
"# cd /var/lib/cvs\n"
"# chown -R root:src repository\n"
"# chmod -R ug+rwX   repository\n"
"# chmod    2775     repository"
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25621
msgid "Execution bit"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25622
msgid ""
"A file's execution bit is retained when checked out.  Whenever you see "
"execution permission problems in checked-out files, change permissions of "
"the file in the CVS repository with the following command."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25623
#, no-wrap
msgid "# chmod ugo-x filename"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:25628
msgid ""
"Subversion is a <emphasis role=\"strong\">next-generation</emphasis> version "
"control system, intended to replace CVS, so it has most of CVS's features. "
"Generally, Subversion's interface to a particular feature is similar to "
"CVS's, except where there's a compelling reason to do otherwise."
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25630
msgid "Installing a Subversion server"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25631
msgid ""
"You need to install <literal>subversion</literal>, <literal>libapache2-svn</"
"literal> and <literal>subversion-tools</literal> packages to set up a server."
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25634
msgid "Setting up a repository"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25635
msgid ""
"Currently, the <literal>subversion</literal> package does not set up a "
"repository, so one must be set up manually.  One possible location for a "
"repository is in \"<literal>/var/local/repos</literal>\"."
msgstr ""

#: debian-reference.en.xml:25636
msgid "Create the directory:"
msgstr "ディレクトリ作成"

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25637
#, no-wrap
msgid "# mkdir -p /var/local/repos"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25638
msgid "Create the repository database:"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25639
#, no-wrap
msgid "# svnadmin create /var/local/repos"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25640
msgid "Make the repository writable by the WWW server:"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25641
#, no-wrap
msgid "# chown -R www-data:www-data /var/local/repos"
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25644
msgid "Configuring Apache2"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25645
msgid ""
"To allow access to the repository via user authentication, add (or "
"uncomment) the following in \"<literal>/etc/apache2/mods-available/dav_svn."
"conf</literal>\":"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25646
#, no-wrap
msgid ""
"&lt;Location /repos&gt;\n"
"  DAV svn\n"
"  SVNPath /var/local/repos\n"
"  AuthType Basic\n"
"  AuthName \"Subversion repository\"\n"
"  AuthUserFile /etc/subversion/passwd\n"
"&lt;LimitExcept GET PROPFIND OPTIONS REPORT&gt;\n"
"    Require valid-user\n"
"&lt;/LimitExcept&gt;\n"
"&lt;/Location&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25656
msgid "Then, create a user authentication file with the command:"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25657
#, no-wrap
msgid "# htpasswd2 -c /etc/subversion/passwd some-username"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25658
msgid ""
"Restart Apache2, and your new Subversion repository will be accessible with "
"the URL \"<literal>http://hostname/repos</literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25661
msgid "Subversion usage examples"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25662
msgid ""
"The following sections teach you how to use different commands in Subversion."
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25665
msgid "Create a new Subversion archive"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25666
msgid "To create a new Subversion archive, type the following:"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25667
#, no-wrap
msgid ""
"$ cd ~/your-project         # go to your source directory\n"
"$ svn import http://localhost/repos your-project project-name -m \"initial project import\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25669
msgid ""
"This creates a directory named project-name in your Subversion repository "
"which contains your project files.  Look at \"<literal>http://localhost/"
"repos/</literal>\" to see if it's there."
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25672
msgid "Working with Subversion"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25673
msgid "Working with project-y using Subversion:"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25674
#, no-wrap
msgid ""
"$ mkdir -p /path/to ;cd  /path/to\n"
"$ svn co http://localhost/repos/project-y"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25679
msgid "Check out sources"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25683
#, no-wrap
msgid "$ cd project-y"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25687
msgid "do some work …"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25691
#, no-wrap
msgid "$ svn diff"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25692
msgid "-similar to \"<literal>diff -u repository/ local/</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25693
#, no-wrap
msgid "$ svn revert modified_file"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25701
#, no-wrap
msgid "$ svn ci -m \"Describe changes\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25705
msgid "check in your changes to the repository"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25709
#, no-wrap
msgid ""
"$ vi newfile_added\n"
"$ svn add newfile_added\n"
"$ svn add new_dir"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25715
msgid "recursively add all files in new_dir"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25719
#, no-wrap
msgid "$ svn add -N new_dir2"
msgstr ""

#: debian-reference.en.xml:25723
msgid "non recursively add the directory"
msgstr "非再帰的にディレクトリを追加"

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25727
#, no-wrap
msgid ""
"$ svn ci -m \"Added newfile_added, new_dir, new_dir2\"\n"
"$ svn up"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25732
msgid "merge in latest version from repository"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25736
#, no-wrap
msgid "$ svn log"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25740
msgid "shows all changes committed"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25744
#, no-wrap
msgid ""
"$ svn copy http://localhost/repos/project-y \\\n"
"      http://localhost/repos/project-y-branch \\\n"
"      -m \"creating my branch of project-y\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25750
msgid "branching project-y"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25754
#, no-wrap
msgid ""
"$ svn copy http://localhost/repos/project-y \\\n"
"      http://localhost/repos/projct-y-release1.0 \\\n"
"      -m \"project-y 1.0 release\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25760
msgid "added release tag."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25765
msgid ""
"note that branching and tagging are the same. The only difference is that "
"branches get committed whereas tags do not."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25770
msgid "make changes to branch …"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25774
#, no-wrap
msgid ""
"$ svn merge http://localhost/repos/project-y \\\n"
"   http://localhost/repos/project-y-branch"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25779
msgid "merge branched copy back to main copy"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25783
#, no-wrap
msgid "$ svn co -r 4 http://localhost/repos/project-y"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:25787
msgid "get revision 4"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:25795
msgid ""
"Git can do everything for both local and remote source code management.  "
"This means that you can record the source code changes without needing "
"network connectivity to the remote repository."
msgstr ""

# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:25797
msgid "Before using Git"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25798
msgid ""
"You may wish to set several global configuration in \"<literal>~/.gitconfig</"
"literal>\" such as your name and email address used by Git:"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25799
#, no-wrap
msgid ""
"$ git config --global user.name \"Name Surname\"\n"
"$ git config --global user.email yourname@example.com"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25801
msgid ""
"If you are too used to CVS or Subversion commands, you may wish to set "
"several command aliases;"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25802
#, no-wrap
msgid ""
"$ git config --global alias.ci \"commit -a\"\n"
"$ git config --global alias.co checkout"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:25804
msgid "You can check your global configuration by:"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:25805
#, no-wrap
msgid "$ git config --global --list"
msgstr ""

