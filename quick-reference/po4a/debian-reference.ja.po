#, no-wrap
msgid "$ cvs ci -m \"Describe change\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26064
msgid "save local sources to CVS"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26068
#, no-wrap
msgid ""
"$ vi newfile_added\n"
"$ cvs add newfile_added\n"
"$ cvs ci -m \"Added newfile_added\"\n"
"$ cvs up"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26075
msgid "merge latest version from CVS."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26080
msgid ""
"To create all newly created subdirectories from CVS, use \"<literal>cvs up -"
"d -P</literal>\" instead."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26085
msgid ""
"Watch out for lines starting with \"<literal>C filename</literal>\" which "
"indicates conflicting changes."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26090
msgid "unmodified code is moved to .#filename.version ."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26095
msgid ""
"search for \"<literal>&lt;&lt;&lt;&lt;&lt;&lt;&lt;</literal>\" and "
"\"<literal>&gt;&gt;&gt;&gt;&gt;&gt;&gt;</literal>\" in the files for "
"conflicting changes."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26100
msgid "edit file to fix conflicts."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26104
#, no-wrap
msgid "$ cvs tag Release-1"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26108
msgid "add release tag"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26113
msgid "edit further …"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26117
#, no-wrap
msgid "$ cvs tag -d Release-1"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26121
msgid "remove release tag"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26125
#, no-wrap
msgid ""
"$ cvs ci -m \"more comments\"\n"
"$ cvs tag Release-1"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:26127
msgid "* re-add release tag"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26129
#, no-wrap
msgid ""
"$ cd /path/to\n"
"$ cvs co -r Release-initial -d old project-x"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26134
msgid "get original version to \"<literal>/path/to/old</literal>\" directory"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26138
#, no-wrap
msgid ""
"$ cd old\n"
"$ cvs tag -b Release-initial-bugfixes"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26143
msgid "create branch (-b) tag \"<literal>Release-initial-bugfixes</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26148
msgid "now you can work on the old version (Tag is sticky)"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26152
#, no-wrap
msgid "$ cvs update -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26156
msgid "don't create empty directories"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26161
msgid "source tree now has sticky tag \"Release-initial-bugfixes\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26166
msgid "work on this branch … while someone else making changes too"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26170
#, no-wrap
msgid "$ cvs up -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26174
msgid "sync with files modified by others on this branch"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26178
#, no-wrap
msgid ""
"$ cvs ci -m \"check into this branch\"\n"
"$ cvs update -kk -A -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26183
msgid "remove sticky tag and forget contents"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26188
msgid "update from main trunk without keyword expansion"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26192
#, no-wrap
msgid "$ cvs update -kk -d -P -j Release-initial-bugfixes"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26196
msgid "merge from Release-initial-bugfixes branch into the main"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26201
msgid "trunk without keyword expansion.  Fix conflicts with editor."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26205
#, no-wrap
msgid ""
"$ cvs ci -m \"merge Release-initial-bugfixes\"\n"
"$ cd\n"
"$ tar -cvzf old-project-x.tar.gz old"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26211
msgid ""
"make archive. use \"<literal>-j</literal>\" if you want \"<literal>.tar.bz2</"
"literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26215
#, no-wrap
msgid "$ cvs release -d old"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26219
msgid "remove local source (optional)"
msgstr ""

