# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:27096
#, no-wrap
msgid "&lt;name attribute=\"value\"&gt;content&lt;/name&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:27097
msgid "XML element with empty content is marked up in the short form as:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:27098
#, no-wrap
msgid "&lt;name attribute=\"value\"/&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:27099
msgid ""
"The \"<literal>attribute=\"value\"</literal>\" in the above examples are "
"optional."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:27100
msgid "The comment section in XML is marked up as:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:27101
#, no-wrap
msgid "&lt;!-- comment --&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:27102
msgid ""
"Other than adding markups, XML requires minor conversion to the content "
"using predefined entities for the following character:"
