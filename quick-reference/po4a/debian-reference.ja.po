msgid "<literal>/</literal>"
msgstr ""

#: debian-reference.en.xml:822
msgid "A simple \"<literal>/</literal>\" represents the root directory."
msgstr "\"<literal>/</literal>\"だけだとrootディレクトリを表します。"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:827
msgid "<literal>/etc/</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
