msgid "For example, try the following"
msgstr "例えば、次を試してみて下さい:"

# type: Content of: <book><chapter><section><section><screen>
#. type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:1834
#, no-wrap
msgid ""
"$ touch foo bar\n"
"$ chmod u=rw,go=r foo\n"
"$ chmod 644 bar\n"
"$ ls -l foo bar\n"
"-rw-r--r-- 1 penguin penguin 17 2007-04-29 08:22 bar\n"
"-rw-r--r-- 1 penguin penguin 12 2007-04-29 08:22 foo"
msgstr ""
"$ touch foo bar\n"
"$ chmod u=rw,go=r foo\n"
"$ chmod 644 bar\n"
"$ ls -l foo bar\n"
