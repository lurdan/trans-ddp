# type: Content of: <book><chapter><section><section><section><title>
#: debian-reference.en.xml:26026
msgid "Work with CVS"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:26027
msgid ""
"To work with <literal>project-x</literal> using the local CVS repository:"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26029
#, no-wrap
msgid ""
"$ mkdir -p /path/to; cd /path/to\n"
"$ cvs co project-x"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26034
msgid "get sources from CVS to local"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26038
#, no-wrap
msgid "$ cd project-x"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26042 debian-reference.en.xml:26289
msgid "make changes to the content …"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26046
#, no-wrap
msgid "$ cvs diff -u"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26050
msgid "similar to \"<literal>diff -u repository/ local/</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26054
#, no-wrap
msgid "$ cvs up -C modified_file"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26058 debian-reference.en.xml:26397
msgid "undo changes to a file"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26062
#, no-wrap
msgid "$ cvs ci -m \"Describe change\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26066
msgid "save local sources to CVS"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26070
#, no-wrap
msgid ""
"$ vi newfile_added\n"
"$ cvs add newfile_added\n"
"$ cvs ci -m \"Added newfile_added\"\n"
"$ cvs up"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26077
msgid "merge latest version from CVS."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26082
msgid ""
"To create all newly created subdirectories from CVS, use \"<literal>cvs up -"
"d -P</literal>\" instead."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26087
msgid ""
"Watch out for lines starting with \"<literal>C filename</literal>\" which "
"indicates conflicting changes."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26092
msgid "unmodified code is moved to .#filename.version ."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26097
msgid ""
"search for \"<literal>&lt;&lt;&lt;&lt;&lt;&lt;&lt;</literal>\" and "
"\"<literal>&gt;&gt;&gt;&gt;&gt;&gt;&gt;</literal>\" in the files for "
"conflicting changes."
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26102
msgid "edit file to fix conflicts."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26106
#, no-wrap
msgid "$ cvs tag Release-1"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26110
msgid "add release tag"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26115
msgid "edit further …"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26119
#, no-wrap
msgid "$ cvs tag -d Release-1"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26123
msgid "remove release tag"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26127
#, no-wrap
msgid ""
"$ cvs ci -m \"more comments\"\n"
"$ cvs tag Release-1"
msgstr ""

# type: Content of: <book><chapter><section><section><section><simpara>
#: debian-reference.en.xml:26129
msgid "* re-add release tag"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26131
#, no-wrap
msgid ""
"$ cd /path/to\n"
"$ cvs co -r Release-initial -d old project-x"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26136
msgid "get original version to \"<literal>/path/to/old</literal>\" directory"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26140
#, no-wrap
msgid ""
"$ cd old\n"
"$ cvs tag -b Release-initial-bugfixes"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26145
msgid "create branch (-b) tag \"<literal>Release-initial-bugfixes</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26150
msgid "now you can work on the old version (Tag is sticky)"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26154
#, no-wrap
msgid "$ cvs update -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26158
msgid "don't create empty directories"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26163
msgid "source tree now has sticky tag \"Release-initial-bugfixes\""
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26168
msgid "work on this branch … while someone else making changes too"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26172
#, no-wrap
msgid "$ cvs up -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26176
msgid "sync with files modified by others on this branch"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26180
#, no-wrap
msgid ""
"$ cvs ci -m \"check into this branch\"\n"
"$ cvs update -kk -A -d -P"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26185
msgid "remove sticky tag and forget contents"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26190
msgid "update from main trunk without keyword expansion"
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26194
#, no-wrap
msgid "$ cvs update -kk -d -P -j Release-initial-bugfixes"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26198
msgid "merge from Release-initial-bugfixes branch into the main"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26203
msgid "trunk without keyword expansion.  Fix conflicts with editor."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26207
#, no-wrap
msgid ""
"$ cvs ci -m \"merge Release-initial-bugfixes\"\n"
"$ cd\n"
"$ tar -cvzf old-project-x.tar.gz old"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26213
msgid ""
"make archive. use \"<literal>-j</literal>\" if you want \"<literal>.tar.bz2</"
"literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><section><screen>
#: debian-reference.en.xml:26217
#, no-wrap
msgid "$ cvs release -d old"
msgstr ""

# type: Content of: <book><chapter><section><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:26221
msgid "remove local source (optional)"
