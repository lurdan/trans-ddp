# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:1530 debian-reference.en.xml:1541
msgid "<literal>/dev/null</literal>"
msgstr "<literal>/dev/null</literal>"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:1533 debian-reference.en.xml:1555
#: debian-reference.en.xml:1566 debian-reference.en.xml:1577
