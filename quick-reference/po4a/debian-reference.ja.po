msgstr "<xref linkend=\"_unix_like_filesystem\"/>（基本コンセプト）。"

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:727
msgid "<xref linkend=\"_midnight_commander_mc\"/> (survival method),"
msgstr "<xref linkend=\"_midnight_commander_mc\"/>（サバイバル手法）。"

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:732
msgid ""
"<xref linkend=\"_the_basic_unix_like_work_environment\"/> (basic method),"
msgstr ""
"<xref linkend=\"_the_basic_unix_like_work_environment\"/>（基本手法）。"

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:737
