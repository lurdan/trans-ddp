msgstr "<literal> | head -n 2 -</literal>"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4261
msgid "print the first 2 lines."
msgstr "最初の2行を表示します。"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4266
