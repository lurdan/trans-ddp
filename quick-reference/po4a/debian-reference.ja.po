msgid "<literal>$</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:3977
msgid "This matches the end of a string."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:3982 debian-reference.en.xml:3985
msgid "<literal>\\&lt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:3988
msgid "This matches the beginning of a word."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:3993 debian-reference.en.xml:3996
msgid "<literal>\\&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:3999
msgid "This matches the end of a word."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4004
msgid "<literal>\\[abc…\\]</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4007
msgid "<literal>[abc…]</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4010
msgid ""
"This character list matches any of the characters \"<literal>abc…</literal>"
"\"."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4015
msgid "<literal>\\[^abc…\\]</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4018
msgid "<literal>[^abc…]</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4021
msgid ""
"This negated character list matches any of the characters except "
"\"<literal>abc…</literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4026 debian-reference.en.xml:4029
msgid "<literal>r*</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4032
msgid ""
"This matches zero or more regular expressions identified by \"<literal>r</"
"literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4037
msgid "<literal>r\\+</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4040
msgid "<literal>r+</literal> T"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4043
msgid ""
"his matches one or more regular expressions identified by \"<literal>r</"
"literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4048
msgid "<literal>r\\?</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4051
msgid "<literal>r?</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4054
msgid ""
"This matches zero or one regular expressions identified by \"<literal>r</"
"literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4059
msgid "<literal>r1\\|r2</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4062
msgid "<literal>r1|r2</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4065
msgid ""
"This matches one of the regular expressions identified by \"<literal>r1</"
"literal>\" or \"<literal>r2</literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4070
msgid "<literal>\\(r1\\|r2\\)</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4073
msgid "<literal>(r1|r2)</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4076
msgid ""
"This matches one of the regular expressions identified by \"<literal>r1</"
"literal>\" or \"<literal>r2</literal>\" and treats it as a <emphasis role="
"\"strong\">bracketed</emphasis> regular expression."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4082
msgid ""
"The regular expression of <emphasis role=\"strong\"><literal>emacs</"
"literal></emphasis> is basically <emphasis role=\"strong\">BRE</emphasis> "
"but has been extended to treat \"<literal>+</literal>\"and \"<literal>?</"
"literal>\" as the <emphasis role=\"strong\">metacharacters</emphasis> as in "
"<emphasis role=\"strong\">ERE</emphasis>.  Thus, there are no needs to quote "
"them with \"<literal>\\</literal>\" in the regular expression of "
"<literal>emacs</literal>."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4084
msgid ""
"For example, <literal>grep</literal>(1) can be used to perform the text "
"search using the regular expression:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4086
#, no-wrap
msgid ""
"$ egrep 'GNU.*LICENSE|Yoyodyne' /usr/share/common-licenses/GPL\n"
"GNU GENERAL PUBLIC LICENSE\n"
"GNU GENERAL PUBLIC LICENSE\n"
"Yoyodyne, Inc., hereby disclaims all copyright interest in the program"
msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xml:4093
msgid "Replacement expressions"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4094
msgid ""
"For the replacement expression, following characters have special meanings:"
msgstr ""

# type: Content of: <book><chapter><section><section><table><title>
#: debian-reference.en.xml:4097
msgid "The replacement expression."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4114
msgid "<literal>&amp;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4117
msgid ""
"This represents what the regular expression matched.  (use <literal>\\&amp;</"
"literal> in <literal>emacs</literal>)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4122
msgid "<literal>\\n</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4125
msgid ""
"This represents what the n-th <emphasis role=\"strong\">bracketed</emphasis> "
"regular expression matched. (\"n\" being number)"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4131
msgid ""
"For Perl replacement string, \"<literal>$n</literal>\" is used instead of "
"\"<literal>\\n</literal>\" and \"<literal>&amp;</literal>\" has no special "
"meaning."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4133
msgid "For example:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4135
#, no-wrap
msgid ""
"$ echo zzz1abc2efg3hij4 | \\\n"
"sed -e 's/\\(1[a-z]*\\)[0-9]*\\(.*\\)$/=&amp;=/'\n"
"zzz=1abc2efg3hij4=\n"
"$ echo zzz1abc2efg3hij4 | \\\n"
"sed -e 's/\\(1[a-z]*\\)[0-9]*\\(.*\\)$/\\2===\\1/'\n"
"zzzefg3hij4===1abc\n"
"$ echo zzz1abc2efg3hij4 | \\\n"
"perl -pe 's/(1[a-z]*)[0-9]*(.*)$/$2===$1/'\n"
"zzzefg3hij4===1abc\n"
"$ echo zzz1abc2efg3hij4 | \\\n"
"perl -pe 's/(1[a-z]*)[0-9]*(.*)$/=&amp;=/'\n"
"zzz=&amp;="
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4147
msgid ""
"Here please pay extra attention to the style of the <emphasis role=\"strong"
"\">bracketed</emphasis> regular expression and how the matched strings are "
"used in the text replacement process on different tools."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4149
msgid ""
"These regular expressions can be used for the cursor movements and the text "
"replacement actions in the editors too."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4151
msgid ""
"The back slash \"<literal>\\</literal>\" at the end of line in the shell "
"commandline escapes newline as a white space character and continues shell "
"command line input to the next line."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4153
msgid "Please read all the related manual pages to learn these commands."
msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xml:4157
msgid "Global substitution with regular expressions"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4158
msgid ""
"The <literal>ed</literal>(1) command can replace all instances of "
"\"<literal>FROM_REGEX</literal>\" with \"<literal>TO_TEXT</literal>\" in "
"\"<literal>file</literal>\" by:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4160
#, no-wrap
msgid ""
"$ ed file &lt;&lt;EOF\n"
",s/FROM_REGEX/TO_TEXT/g\n"
"w\n"
"q\n"
"EOF"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4165
msgid ""
"The <literal>vim</literal>(1) command can replace all instances of "
"\"<literal>FROM_REGEX</literal>\" with \"<literal>TO_TEXT</literal>\" in "
"\"<literal>file</literal>\" by using <literal>ex</literal>(1) commands:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4167
#, no-wrap
msgid "$ vim '+%s/FROM_REGEX/TO_TEXT/gc' '+w' '+q' file"
msgstr ""

# type: Content of: <book><chapter><section><section><tip><simpara>
#: debian-reference.en.xml:4168
msgid ""
"The \"<literal>c</literal>\" flag in the above ensures interactive "
"confirmation for each substitution."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4169
msgid ""
"Multiple files (\"<literal>file1</literal>\", \"<literal>file2</literal>\", "
"and \"<literal>file3</literal>\") can be processed with regular expressions "
"similarly with <literal>vim</literal>(1) or <literal>perl</literal>(1):"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4171
#, no-wrap
msgid "$ vim '+argdo %s/FROM_REGEX/TO_TEXT/g|update' '+q' file1 file2 file3"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4172
#, no-wrap
msgid "$ perl -i -p -e 's/FROM_REGEX/TO_TEXT/g;' file1 file2 file3"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4173
msgid ""
"In the perl(1) example, \"<literal>-i</literal>\" is for in-place editing, "
"\"<literal>-p</literal>\" is for implicit loop over files.  If the "
"substitution is complex, you can make recovery from errors easier by using "
"the parameter \"<literal>-i.bak</literal>\" instead of \"<literal>-i</"
"literal>\"; this will keep each original file, adding \"<literal>.bak</"
"literal>\" as a file extension."
msgstr ""

# type: Content of: <book><chapter><section><section><note><simpara>
#: debian-reference.en.xml:4175
msgid ""
"While <literal>ed</literal>(1) and <literal>vim</literal>(1) are <emphasis "
"role=\"strong\">BRE</emphasis>, <literal>perl</literal>(1) is <emphasis role="
"\"strong\">ERE</emphasis>."
msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xml:4178
msgid "Extract data from text file table"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4179
msgid ""
"Let's consider a text file called <literal>DPL</literal> in which some pre-"
"2004 Debian project leader's names and their initiation days are listed in a "
"space-separated format."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4183
#, no-wrap
msgid ""
"Ian     Murdock   August  1993\n"
"Bruce   Perens    April   1996\n"
"Ian     Jackson   January 1998\n"
"Wichert Akkerman  January 1999\n"
"Ben     Collins   April   2001\n"
"Bdale   Garbee    April   2002\n"
"Martin  Michlmayr March   2003"
msgstr ""

# type: Content of: <book><chapter><section><section><tip><simpara>
#: debian-reference.en.xml:4190
msgid ""
"See <ulink url=\"&abriefhistoryofdebian;\">\"A Brief History of Debian\"</"
"ulink> for the latest <ulink url=\"&debianleadershiphistory;\">Debian "
"leadership history</ulink>."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4191
msgid "Awk is frequently used to extract data from these types of files."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4193
#, no-wrap
msgid ""
"$ awk '{ print $3 }' &lt;DPL                   # month started\n"
"August\n"
"April\n"
"January\n"
"January\n"
"April\n"
"April\n"
"March\n"
"$ awk '($1==\"Ian\") { print }' &lt;DPL          # DPL called Ian\n"
"Ian     Murdock   August  1993\n"
"Ian     Jackson   January 1998\n"
"$ awk '($2==\"Perens\") { print $3,$4 }' &lt;DPL # When Perens started\n"
"April 1996"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4206
msgid "Shells such as Bash can be also used to parse this kind of file:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4208
#, no-wrap
msgid ""
"$ while read first last month year; do\n"
"    echo $month\n"
"  done &lt;DPL"
msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xml:4214
msgid "same output as the first Awk example."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4218
msgid ""
"Here, the <literal>read</literal> builtin command uses the characters in "
"\"<literal>$IFS</literal>\" (internal field separators) to split lines into "
"words."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4221
msgid ""
"If you change \"<literal>$IFS</literal>\" to \"<literal>:</literal>\", you "
"can parse \"<literal>/etc/passwd</literal>\" with shell nicely:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4223
#, no-wrap
msgid ""
"$ oldIFS=\"$IFS\"   # save old value\n"
"$ IFS=':'\n"
"$ while read user password uid gid rest_of_line; do\n"
"    if [ \"$user\" = \"bozo\" ]; then\n"
"      echo \"$user's ID is $uid\"\n"
"    fi\n"
"  done &lt; /etc/passwd\n"
"bozo's ID is 1000\n"
"$ IFS=\"$oldIFS\"   # restore old value"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4232
msgid ""
"(If Awk is used to do the equivalent, use \"<literal>FS=':'</literal>\" to "
"set the field separator.)"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4234
msgid ""
"IFS is also used by the shell to split results of parameter expansion, "
"command substitution, and arithmetic expansion.  These do not occur within "
"double or single quoted words.  The default value of IFS is &lt;space&gt;, "
"&lt;tab&gt;, and &lt;newline&gt; combined."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4236
msgid ""
"Be careful about using this shell IFS tricks.  Strange things may happen, "
"when shell interprets some parts of the script as its <emphasis role=\"strong"
"\">input</emphasis>."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xml:4239
#, no-wrap
msgid ""
"$ IFS=\":,\"                        # use \":\" and \",\" as IFS\n"
"$ echo IFS=$IFS,   IFS=\"$IFS\"     # echo is a Bash builtin\n"
"IFS=  , IFS=:,\n"
"$ date -R                         # just a command output\n"
"Sat, 23 Aug 2003 08:30:15 +0200\n"
"$ echo $(date -R)                 # sub shell --&gt; input to main shell\n"
"Sat  23 Aug 2003 08 30 36 +0200\n"
"$ unset IFS                       # reset IFS to the default\n"
"$ echo $(date -R)\n"
"Sat, 23 Aug 2003 08:30:50 +0200"
msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xml:4251
msgid "Script snippets for piping commands"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xml:4252
msgid "The following scripts will do nice things as a part of a pipe."
msgstr ""

# type: Content of: <book><chapter><section><section><table><title>
#: debian-reference.en.xml:4255
msgid "List of script snippets for piping commands."
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xml:4262
msgid "script snippet (type in one line)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xml:4265
msgid "effect"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4272
msgid "<literal>find /usr -print</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4275
msgid ""
"find all files under \"<literal>/usr</literal>\" (see <xref linkend="
"\"_idioms_for_the_selection_of_files\"/>)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4280
msgid "<literal>seq 1 100</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4283
msgid "print 1 to 100"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4288
msgid "<literal> | xargs -n 1 &lt;command&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4291
msgid ""
"run command repeatedly with each item from pipe as its argument (see <xref "
"linkend=\"_repeating_a_command_looping_over_files\"/>)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4296
msgid "<literal> | xargs -n 1 echo</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4299
msgid "split white-space-separated items from pipe into lines"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4304
msgid "<literal> | xargs echo</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4307
msgid "merge all lines from pipe into a line"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4312
msgid "<literal> | grep -e &lt;regex_pattern&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4315
msgid "extract lines from pipe containing &lt;regex_pattern&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4320
msgid "<literal> | grep -v -e &lt;regex_pattern&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4323
msgid "extract lines from pipe not containing &lt;regex_pattern&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4328
msgid "<literal> | cut -d: -f3 -</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4331
msgid ""
"extract third field from pipe separated by \"<literal>:</literal>\" (passwd "
"file etc.)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4336
msgid "<literal> | awk '{ print $3 }'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4339
msgid "extract third field from pipe separated by whitespaces"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4344
msgid "<literal> | awk -F'\\t' '{ print $3 }'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4347
msgid "extract third field from pipe separated by tab"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4352
msgid "<literal> | col -bx</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4355
msgid "remove backspace and expand tabs to spaces"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4360
msgid "<literal> | expand -</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4363
msgid "expand tabs"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4368
msgid "<literal> | sort| uniq</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4371
msgid "sort and remove duplicates"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4376
msgid "<literal> | tr 'A-Z' 'a-z'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4379
msgid "convert uppercase to lowercase"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4384
msgid "<literal> | tr -d '\\n'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4387
msgid "concatenate lines into one line"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4392
msgid "<literal> | tr -d '\\r'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4395
msgid "remove CR"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4400
msgid "<literal> | sed 's/^/# /'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4403
msgid "add \"<literal>#</literal>\" to the start of each line"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4408
msgid "<literal> | sed 's/\\.ext//g'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4411
msgid "remove \"<literal>.ext</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4416
msgid "<literal> | sed -n -e 2p</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4419
msgid "print the second line"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4424
msgid "<literal> | head -n 2 -</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4427
msgid "print the first 2 lines"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4432
msgid "<literal> | tail -n 2 -</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:4435
msgid "print the last 2 lines"
msgstr ""

