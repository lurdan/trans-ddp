msgstr "<literal>stdin</literal>"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:3539
msgid "standard input"
msgstr "標準出力"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xml:3542 debian-reference.en.xml:8895
#: debian-reference.en.xml:19743
msgid "0"
msgstr "0"

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
