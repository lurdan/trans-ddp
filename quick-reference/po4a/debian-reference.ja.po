msgstr "ファイルが所属する<emphasis role=\"strong\">グループ(group)</emphasis>"

#: debian-reference.en.xml:954
msgid ""
"the <emphasis role=\"strong\">size</emphasis> of the file in characters "
"(bytes)"
msgstr "ファイルの<emphasis role=\"strong\">サイズ</emphasis>(文字数、バイト)"

#: debian-reference.en.xml:959
msgid ""
"the <emphasis role=\"strong\">date and time</emphasis> of the file (mtime)"
msgstr "ファイルの<emphasis role=\"strong\">日時</emphasis>(mtime)"

#: debian-reference.en.xml:964
msgid "the <emphasis role=\"strong\">name</emphasis> of the file."
msgstr "ファイルの<emphasis role=\"strong\">名前</emphasis>。"

