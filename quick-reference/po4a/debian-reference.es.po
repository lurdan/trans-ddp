"type = MDA_external\n"
"path = /usr/bin/maildrop\n"
"unixfrom = True\n"
"\n"
"[options]\n"
"verbose = 0\n"
"delete = True\n"
"delivered_to = False\n"
"message_log = ~/.getmail/log/pop3_name.log"
msgstr ""

