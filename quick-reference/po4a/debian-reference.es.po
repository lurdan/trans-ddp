msgid "<literal>.*</literal>"
msgstr ""

msgid "filename (segment) started with \"<literal>.</literal>\""
msgstr ""

msgid "<literal>?</literal>"
msgstr ""

msgid "exactly one character"
msgstr ""

msgid "<literal>[…]</literal>"
msgstr ""

msgid "exactly one character with any character enclosed in brackets"
msgstr ""

msgid "<literal>[a-z]</literal>"
msgstr ""

