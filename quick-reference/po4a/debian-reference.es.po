# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xmlt:20008
msgid "Repeating a command looping over files"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:20009
msgid "There are several ways to repeat a command looping over files matching some condition, e.g. matching glob pattern \"<literal>*.ext</literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xmlt:20013
msgid "Shell for-loop method (see <xref linkend=\"_shell_loops\"/>):"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:20017
#, no-wrap
msgid "for x in *.ext; do if [ -f \"$x\"]; then command \"$x\" ; fi; done"
msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xmlt:20020
msgid "<literal>find</literal>(1) and <literal>xargs</literal>(1) combination:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:20024
#, no-wrap
msgid "find . -type f -maxdepth 1 -name '*.ext' -print0 | xargs -0 -n 1 command"
msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xmlt:20027
msgid "<literal>find</literal>(1) with \"<literal>-exec</literal>\" option with a command:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:20031
#, no-wrap
msgid "find . -type f -maxdepth 1 -name '*.ext' -exec command '{}' \\;"
msgstr ""

# type: Content of: <book><chapter><section><section><itemizedlist><listitem><simpara>
#: debian-reference.en.xmlt:20034
msgid "<literal>find</literal>(1) with \"<literal>-exec</literal>\" option with a short shell script:"
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:20038
#, no-wrap
msgid "find . -type f -maxdepth 1 -name '*.ext' -exec sh -c \"command '{}' &amp;&amp; echo 'successful'\" \\;"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:20039
msgid "The above examples are written to ensure proper handling of funny file names such as ones containing spaces.  See <xref linkend=\"_idioms_for_the_selection_of_files\"/> for more advance uses of <literal>find</literal>(1)."
msgstr ""

# type: Content of: <book><chapter><section><section><title>
