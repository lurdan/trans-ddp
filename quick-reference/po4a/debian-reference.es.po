msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xmlt:4440
msgid "Regular expressions"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4441
msgid "<ulink url=\"&regularexpressions;\">Regular expressions</ulink> are used in many text processing tools.  They are analogous to the shell globs, but they are more complicated and powerful."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4442
msgid "The regular expression describes the matching pattern and is made up of text characters and <emphasis role=\"strong\">metacharacters</emphasis>."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4443
msgid "The <emphasis role=\"strong\">metacharacter</emphasis> is just a character with a special meaning.  There are 2 major styles, <emphasis role=\"strong\">BRE</emphasis> and <emphasis role=\"strong\">ERE</emphasis>, depending on the text tools as described above."
msgstr ""

# type: Content of: <book><chapter><section><section><table><title>
#: debian-reference.en.xmlt:4445
msgid "Metacharacters for BRE and ERE"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:4453
msgid "BRE"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:4456
msgid "ERE"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:4459
msgid "description of the regular expression"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4466
msgid "<literal> \\ .  [ ] ^ $ * </literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4469
msgid "<literal>\\ .  [ ] ^ $ *</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4472
msgid "common <emphasis role=\"strong\">metacharacters</emphasis>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4477
msgid "<literal>\\+ \\? \\( \\) \\{ \\} \\|</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4480 debian-reference.en.xmlt:4488
msgid " "
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4483
msgid "BRE only \"<literal>\\</literal>\" escaped <emphasis role=\"strong\">metacharacters</emphasis>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4491
msgid "<literal>+ ? ( ) { } |</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4494
msgid "ERE only non-\"<literal>\\</literal>\" escaped <emphasis role=\"strong\">metacharacters</emphasis>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4505
msgid "match <emphasis role=\"strong\">non-metacharacter</emphasis> \"<literal>c</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4510 debian-reference.en.xmlt:4513
msgid "<literal>\\c</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4516
msgid "match a literal character \"<literal>c</literal>\" even if \"<literal>c</literal>\" is <emphasis role=\"strong\">metacharacter</emphasis> by itself"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4521 debian-reference.en.xmlt:4524
msgid "<literal>.</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4527
msgid "match any character including newline"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4532 debian-reference.en.xmlt:4535
msgid "<literal>^</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4538
msgid "position at the beginning of a string"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4543 debian-reference.en.xmlt:4546
msgid "<literal>$</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4549
msgid "position at the end of a string"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4554 debian-reference.en.xmlt:4557
msgid "<literal>\\&lt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4560
msgid "position at the beginning of a word"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4565 debian-reference.en.xmlt:4568
msgid "<literal>\\&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4571
msgid "position at the end of a word"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4576
msgid "<literal>\\[abc…\\]</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4579
msgid "<literal>[abc…]</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4582
msgid "match any characters in \"<literal>abc…</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4587
msgid "<literal>\\[^abc…\\]</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4590
msgid "<literal>[^abc…]</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4593
msgid "match any characters except in \"<literal>abc…</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4598 debian-reference.en.xmlt:4601
msgid "<literal>r*</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4604
msgid "match zero or more regular expressions identified by \"<literal>r</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4609
msgid "<literal>r\\+</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4612
msgid "<literal>r+</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4615
msgid "match one or more regular expressions identified by \"<literal>r</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4620
msgid "<literal>r\\?</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4623
msgid "<literal>r?</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4626
msgid "match zero or one regular expressions identified by \"<literal>r</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4631
msgid "<literal>r1\\|r2</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4634
msgid "<literal>r1|r2</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4637
msgid "match one of the regular expressions identified by \"<literal>r1</literal>\" or \"<literal>r2</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4642
msgid "<literal>\\(r1\\|r2\\)</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4645
msgid "<literal>(r1|r2)</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4648
msgid "match one of the regular expressions identified by \"<literal>r1</literal>\" or \"<literal>r2</literal>\" and treat it as a <emphasis role=\"strong\">bracketed</emphasis> regular expression"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4654
msgid "The regular expression of <emphasis role=\"strong\"><literal>emacs</literal></emphasis> is basically <emphasis role=\"strong\">BRE</emphasis> but has been extended to treat \"<literal>+</literal>\"and \"<literal>?</literal>\" as the <emphasis role=\"strong\">metacharacters</emphasis> as in <emphasis role=\"strong\">ERE</emphasis>.  Thus, there are no needs to escape them with \"<literal>\\</literal>\" in the regular expression of <literal>emacs</literal>."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4655
msgid "<literal>grep</literal>(1) can be used to perform the text search using the regular expression."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4657
#, no-wrap
msgid ""
"$ egrep 'GNU.*LICENSE|Yoyodyne' /usr/share/common-licenses/GPL\n"
"GNU GENERAL PUBLIC LICENSE\n"
"GNU GENERAL PUBLIC LICENSE\n"
"Yoyodyne, Inc., hereby disclaims all copyright interest in the program"
msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xmlt:4666
msgid "Replacement expressions"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4667
msgid "For the replacement expression, some characters have special meanings."
msgstr ""

# type: Content of: <book><chapter><section><section><table><title>
#: debian-reference.en.xmlt:4669
msgid "The replacement expression"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:4676
msgid "replacement expression"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:4679
msgid "description of the text to replace the replacement expression"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4686
msgid "<literal>&amp;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4689
msgid "what the regular expression matched (use <literal>\\&amp;</literal> in <literal>emacs</literal>)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4694
msgid "<literal>\\n</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4697
msgid "what the n-th <emphasis role=\"strong\">bracketed</emphasis> regular expression matched (\"n\" being number)"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4703
msgid "For Perl replacement string, \"<literal>$n</literal>\" is used instead of \"<literal>\\n</literal>\" and \"<literal>&amp;</literal>\" has no special meaning."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4705
#, no-wrap
msgid ""
"$ echo zzz1abc2efg3hij4 | \\\n"
"sed -e 's/\\(1[a-z]*\\)[0-9]*\\(.*\\)$/=&amp;=/'\n"
"zzz=1abc2efg3hij4=\n"
"$ echo zzz1abc2efg3hij4 | \\\n"
"sed -e 's/\\(1[a-z]*\\)[0-9]*\\(.*\\)$/\\2===\\1/'\n"
"zzzefg3hij4===1abc\n"
"$ echo zzz1abc2efg3hij4 | \\\n"
"perl -pe 's/(1[a-z]*)[0-9]*(.*)$/$2===$1/'\n"
"zzzefg3hij4===1abc\n"
"$ echo zzz1abc2efg3hij4 | \\\n"
"perl -pe 's/(1[a-z]*)[0-9]*(.*)$/=&amp;=/'\n"
"zzz=&amp;="
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4717
msgid "Here please pay extra attention to the style of the <emphasis role=\"strong\">bracketed</emphasis> regular expression and how the matched strings are used in the text replacement process on different tools."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4718
msgid "These regular expressions can be used for cursor movements and text replacement actions in some editors too."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4719
msgid "The back slash \"<literal>\\</literal>\" at the end of line in the shell commandline escapes newline as a white space character and continues shell command line input to the next line."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4720
msgid "Please read all the related manual pages to learn these commands."
msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xmlt:4723
msgid "Global substitution with regular expressions"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4724
msgid "The <literal>ed</literal>(1) command can replace all instances of \"<literal>FROM_REGEX</literal>\" with \"<literal>TO_TEXT</literal>\" in \"<literal>file</literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4725
#, no-wrap
msgid ""
"$ ed file &lt;&lt;EOF\n"
",s/FROM_REGEX/TO_TEXT/g\n"
"w\n"
"q\n"
"EOF"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4730
msgid "The <literal>sed</literal>(1) command can replace all instances of \"<literal>FROM_REGEX</literal>\" with \"<literal>TO_TEXT</literal>\" in \"<literal>file</literal>\"."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4731
#, no-wrap
msgid "$ sed file 's/FROM_REGEX/TO_TEXT/g' | sponge file"
msgstr ""

# type: Content of: <book><chapter><section><section><tip><simpara>
#: debian-reference.en.xmlt:4733
msgid "The <literal>sponge</literal>(8) command is a non-standard Unix tool offered by the <literal>moreutils</literal> package.  This is quite useful when you wish to overwrite original file."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4735
msgid "The <literal>vim</literal>(1) command can replace all instances of \"<literal>FROM_REGEX</literal>\" with \"<literal>TO_TEXT</literal>\" in \"<literal>file</literal>\" by using <literal>ex</literal>(1) commands."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4736
#, no-wrap
msgid "$ vim '+%s/FROM_REGEX/TO_TEXT/gc' '+w' '+q' file"
msgstr ""

# type: Content of: <book><chapter><section><section><tip><simpara>
#: debian-reference.en.xmlt:4738
msgid "The \"<literal>c</literal>\" flag in the above ensures interactive confirmation for each substitution."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4740
msgid "Multiple files (\"<literal>file1</literal>\", \"<literal>file2</literal>\", and \"<literal>file3</literal>\") can be processed with regular expressions similarly with <literal>vim</literal>(1) or <literal>perl</literal>(1)."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4741
#, no-wrap
msgid "$ vim '+argdo %s/FROM_REGEX/TO_TEXT/ge|update' '+q' file1 file2 file3"
msgstr ""

# type: Content of: <book><chapter><section><section><tip><simpara>
#: debian-reference.en.xmlt:4743
msgid "The \"<literal>e</literal>\" flag in the above prevents the \"No match\" error from breaking a mapping."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4745
#, no-wrap
msgid "$ perl -i -p -e 's/FROM_REGEX/TO_TEXT/g;' file1 file2 file3"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4746
msgid "In the perl(1) example, \"<literal>-i</literal>\" is for in-place editing, \"<literal>-p</literal>\" is for implicit loop over files."
msgstr ""

# type: Content of: <book><chapter><section><section><tip><simpara>
#: debian-reference.en.xmlt:4748
msgid "Use of argument \"<literal>-i.bak</literal>\" instead of \"<literal>-i</literal>\" keeps each original file by adding \"<literal>.bak</literal>\" to its filename.  This makes recovery from errors easier for complex substitutions."
msgstr ""

# type: Content of: <book><chapter><section><section><note><simpara>
#: debian-reference.en.xmlt:4751
msgid "<literal>ed</literal>(1) and <literal>vim</literal>(1) are <emphasis role=\"strong\">BRE</emphasis>; <literal>perl</literal>(1) is <emphasis role=\"strong\">ERE</emphasis>."
msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xmlt:4755
msgid "Extracting data from text file table"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4756
msgid "Let's consider a text file called \"<literal>DPL</literal>\" in which some pre-2004 Debian project leader's names and their initiation days are listed in a space-separated format."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4758
#, no-wrap
msgid ""
"Ian     Murdock   August  1993\n"
"Bruce   Perens    April   1996\n"
"Ian     Jackson   January 1998\n"
"Wichert Akkerman  January 1999\n"
"Ben     Collins   April   2001\n"
"Bdale   Garbee    April   2002\n"
"Martin  Michlmayr March   2003"
msgstr ""

# type: Content of: <book><chapter><section><section><tip><simpara>
#: debian-reference.en.xmlt:4766
msgid "See <ulink url=\"&abriefhistoryofdebian;\">\"A Brief History of Debian\"</ulink> for the latest <ulink url=\"&debianleadershiphistory;\">Debian leadership history</ulink>."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4768
msgid "Awk is frequently used to extract data from these types of files."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4770
#, no-wrap
msgid ""
"$ awk '{ print $3 }' &lt;DPL                   # month started\n"
"August\n"
"April\n"
"January\n"
"January\n"
"April\n"
"April\n"
"March\n"
"$ awk '($1==\"Ian\") { print }' &lt;DPL          # DPL called Ian\n"
"Ian     Murdock   August  1993\n"
"Ian     Jackson   January 1998\n"
"$ awk '($2==\"Perens\") { print $3,$4 }' &lt;DPL # When Perens started\n"
"April 1996"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4783
msgid "Shells such as Bash can be also used to parse this kind of file."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4785
#, no-wrap
msgid ""
"$ while read first last month year; do\n"
"    echo $month\n"
"  done &lt;DPL\n"
"... same output as the first Awk example"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4789
msgid "Here, the <literal>read</literal> builtin command uses characters in \"<literal>$IFS</literal>\" (internal field separators) to split lines into words."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4790
msgid "If you change \"<literal>$IFS</literal>\" to \"<literal>:</literal>\", you can parse \"<literal>/etc/passwd</literal>\" with shell nicely."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4791
#, no-wrap
msgid ""
"$ oldIFS=\"$IFS\"   # save old value\n"
"$ IFS=':'\n"
"$ while read user password uid gid rest_of_line; do\n"
"    if [ \"$user\" = \"bozo\" ]; then\n"
"      echo \"$user's ID is $uid\"\n"
"    fi\n"
"  done &lt; /etc/passwd\n"
"bozo's ID is 1000\n"
"$ IFS=\"$oldIFS\"   # restore old value"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4800
msgid "(If Awk is used to do the equivalent, use \"<literal>FS=':'</literal>\" to set the field separator.)"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4801
msgid "IFS is also used by the shell to split results of parameter expansion, command substitution, and arithmetic expansion.  These do not occur within double or single quoted words.  The default value of IFS is &lt;space&gt;, &lt;tab&gt;, and &lt;newline&gt; combined."
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4802
msgid "Be careful about using this shell IFS tricks.  Strange things may happen, when shell interprets some parts of the script as its <emphasis role=\"strong\">input</emphasis>."
msgstr ""

# type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4803
#, no-wrap
msgid ""
"$ IFS=\":,\"                        # use \":\" and \",\" as IFS\n"
"$ echo IFS=$IFS,   IFS=\"$IFS\"     # echo is a Bash builtin\n"
"IFS=  , IFS=:,\n"
"$ date -R                         # just a command output\n"
"Sat, 23 Aug 2003 08:30:15 +0200\n"
"$ echo $(date -R)                 # sub shell --&gt; input to main shell\n"
"Sat  23 Aug 2003 08 30 36 +0200\n"
"$ unset IFS                       # reset IFS to the default\n"
"$ echo $(date -R)\n"
"Sat, 23 Aug 2003 08:30:50 +0200"
msgstr ""

# type: Content of: <book><chapter><section><section><title>
#: debian-reference.en.xmlt:4815
msgid "Script snippets for piping commands"
msgstr ""

# type: Content of: <book><chapter><section><section><simpara>
#: debian-reference.en.xmlt:4816
msgid "The following scripts do nice things as a part of a pipe."
msgstr ""

# type: Content of: <book><chapter><section><section><table><title>
#: debian-reference.en.xmlt:4818
msgid "List of script snippets for piping commands"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:4825
msgid "script snippet (type in one line)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:4828
msgid "effect of command"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4835
msgid "<literal>find /usr -print</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4838
msgid "find all files under \"<literal>/usr</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4843
msgid "<literal>seq 1 100</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4846
msgid "print 1 to 100"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4851
msgid "<literal> | xargs -n 1 &lt;command&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4854
msgid "run command repeatedly with each item from pipe as its argument"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4859
msgid "<literal> | xargs -n 1 echo</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4862
msgid "split white-space-separated items from pipe into lines"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4867
msgid "<literal> | xargs echo</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4870
msgid "merge all lines from pipe into a line"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4875
msgid "<literal> | grep -e &lt;regex_pattern&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4878
msgid "extract lines from pipe containing &lt;regex_pattern&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4883
msgid "<literal> | grep -v -e &lt;regex_pattern&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4886
msgid "extract lines from pipe not containing &lt;regex_pattern&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4891
msgid "<literal> | cut -d: -f3 -</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4894
msgid "extract third field from pipe separated by \"<literal>:</literal>\" (passwd file etc.)"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4899
msgid "<literal> | awk '{ print $3 }'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4902
msgid "extract third field from pipe separated by whitespaces"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4907
msgid "<literal> | awk -F'\\t' '{ print $3 }'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4910
msgid "extract third field from pipe separated by tab"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4915
msgid "<literal> | col -bx</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4918
msgid "remove backspace and expand tabs to spaces"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4923
msgid "<literal> | expand -</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4926
msgid "expand tabs"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4931
msgid "<literal> | sort| uniq</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4934
msgid "sort and remove duplicates"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4939
msgid "<literal> | tr 'A-Z' 'a-z'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4942
msgid "convert uppercase to lowercase"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4947
msgid "<literal> | tr -d '\\n'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4950
msgid "concatenate lines into one line"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4955
msgid "<literal> | tr -d '\\r'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4958
msgid "remove CR"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4963
msgid "<literal> | sed 's/^/# /'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4966
msgid "add \"<literal>#</literal>\" to the start of each line"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4971
msgid "<literal> | sed 's/\\.ext//g'</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4974
msgid "remove \"<literal>.ext</literal>\""
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4979
msgid "<literal> | sed -n -e 2p</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4982
msgid "print the second line"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4987
msgid "<literal> | head -n 2 -</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4990
msgid "print the first 2 lines"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4995
msgid "<literal> | tail -n 2 -</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:4998
msgid "print the last 2 lines"
msgstr ""

