#: debian-reference.en.xmlt:9296
msgid "List of runlevels and description of their usage"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:9304
msgid "runlevel"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: debian-reference.en.xmlt:9310
msgid "description of runlevel usage"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:9317
