msgid "match packages from which <emphasis role=\"strong\">broken</emphasis> relation &lt;type&gt; is defined to &lt;term&gt; package"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:6871
msgid "<literal>~DB[&lt;type&gt;:]&lt;term&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:6876
msgid "match packages to which the &lt;term&gt; package defines relation &lt;type&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:6879
msgid "<literal>~R[&lt;type&gt;:]&lt;term&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:6884
msgid "match packages to which the &lt;term&gt; package defines <emphasis role=\"strong\">broken</emphasis> relation &lt;type&gt;"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:6887
msgid "<literal>~RB[&lt;type&gt;:]&lt;term&gt;</literal>"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><tbody><row><entry>
#: debian-reference.en.xmlt:6892
