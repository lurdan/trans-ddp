<!--  ( This is taken from boot-floppy )

Remember, this is SGML; the *first* definition of an entity wins.
Thus file overrules default.ent and is overruled by dynamic.ent.

-->

<!-- update these by each release -->
<!entity version       "Version: 1.00p00">
<!entity date          "Bogus date">

<!-- whether or not to show the FIXME stuff, this set as ignore -->
<!entity % FIXME "IGNORE">

<!-- Keep this comment at the end of the file
Local variables:
mode: sgml
End:
-->
